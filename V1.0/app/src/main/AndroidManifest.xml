<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dan.iteration2" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.dan.iteration2.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.dan.iteration2.GameNumbers"
            android:label="@string/title_activity_game_numbers" >
        </activity>
        <activity
            android:name="com.example.dan.iteration2.GameColors"
            android:label="@string/title_activity_game_colors" >
        </activity>
        <activity
            android:name="com.example.dan.iteration2.GameAnimals"
            android:label="@string/title_activity_game_animals" >
        </activity>
    </application>

</manifest>
